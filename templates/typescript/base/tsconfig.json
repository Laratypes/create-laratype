  "compilerOptions": {
    "target": "ES2022",
    "module": "ES2020",
    "strict": true,
    "jsx": "react-jsx",
    "jsxImportSource": "hono/jsx",
    "lib": ["ES2022"],
    "sourceMap": true,
    "skipLibCheck": true,
    "outDir": "dist",
